<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.weishu.upf.demo.app2">

    <application tools:ignore="GoogleAppIndexingWarning,HardcodedDebugMode">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.weishu.upf.demo.app2.Receiver1">
            <intent-filter>
                <action android:name="com.weishu.upf.demo.app2.Receiver1" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.weishu.upf.demo.app2.Receiver2">
            <intent-filter>
                <action android:name="com.weishu.upf.demo.app2.Receiver2" />
            </intent-filter>
        </receiver>
    </application>
</manifest>